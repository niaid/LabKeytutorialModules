{
  "name": "demo",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "setup": "npm ci",
    "build": "npm run build-dev",
    "start": "cross-env NODE_ENV=development LK_MODULE_CONTAINER=tutorialModules LK_MODULE=demo webpack-dev-server --config node_modules/@labkey/build/webpack/watch.config.js",
    "build-dev": "npm run clean && cross-env NODE_ENV=development LK_MODULE_CONTAINER=tutorialModules LK_MODULE=demo webpack --config node_modules/@labkey/build/webpack/dev.config.js --progress --profile --colors",
    "build-prod": "npm run clean && cross-env NODE_ENV=production PROD_SOURCE_MAP=source-map LK_MODULE_CONTAINER=tutorialModules LK_MODULE=demo webpack --config node_modules/@labkey/build/webpack/prod.config.js --progress --profile --colors",
    "clean": "rimraf resources/web/demo/gen && rimraf resources/views/helloWorld* && rimraf resources/views/todoList*",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "@labkey/api": "1.1.1",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/polyfill": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@hot-loader/react-dom": "16.13.0",
    "@labkey/build": "0.1.0",
    "@types/enzyme": "3.10.7",
    "@types/jest": "26.0.14",
    "@types/react": "16.9.51",
    "@types/react-dom": "16.9.8",
    "@types/react-hot-loader": "4.1.1",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "css-loader": "4.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.5",
    "enzyme-to-json": "3.6.1",
    "file-loader": "6.1.0",
    "jest": "26.5.2",
    "jest-teamcity-reporter": "0.9.0",
    "jest-cli": "26.5.2",
    "html-webpack-plugin": "4.5.0",
    "mini-css-extract-plugin": "0.12.0",
    "node-sass": "4.14.1",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "4.13.0",
    "react-test-renderer": "16.13.1",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "10.0.2",
    "style-loader": "1.3.0",
    "ts-jest": "26.4.1",
    "ts-loader": "8.0.4",
    "typescript": "4.0.3",
    "url-loader": "4.1.0",
    "webpack": "4.44.2",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "jest": {
    "globals": {
      "LABKEY": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/test/styleMock.ts"
    },
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
