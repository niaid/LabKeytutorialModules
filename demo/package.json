{
  "name": "demo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "setup": "npm install",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --config webpack/prod.config.js --progress --colors",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config webpack/dev.config.js",
    "build-prod": "npm run build",
    "clean": "rimraf resources/web/demo/gen && rimraf resources/views/*.*",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "@labkey/api": "1.1.0",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/polyfill": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@hot-loader/react-dom": "16.13.0",
    "@types/enzyme": "3.10.6",
    "@types/jest": "26.0.14",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.8.4",
    "@types/react-hot-loader": "4.1.0",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "css-loader": "4.3.0",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.4",
    "enzyme-to-json": "3.4.0",
    "file-loader": "6.1.0",
    "jest": "26.4.2",
    "jest-teamcity-reporter": "0.9.0",
    "jest-cli": "26.4.2",
    "html-webpack-plugin": "4.4.1",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.14.1",
    "postcss-loader": "3.0.0",
    "react-hot-loader": "4.12.21",
    "react-test-renderer": "16.13.1",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "9.0.3",
    "style-loader": "1.2.1",
    "ts-jest": "26.3.0",
    "ts-loader": "8.0.3",
    "typescript": "4.0.2",
    "url-loader": "4.1.0",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0"
  },
  "jest": {
    "globals": {
      "LABKEY": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.ts"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/test/styleMock.ts"
    },
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  }
}
